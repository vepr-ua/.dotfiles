{
  "language": "en",
  "allowCompoundWords": true,
  "ignorePaths": ["**/node_modules/**", "dist/**", ".git/**"],
  "dictionaryDefinitions": [
    {
      "name": "global-dict",
      "path": "~/.dotfiles/dict/custom-words.txt",
      "addWords": true
    },{
      "name": "local-dict",
      "path": "~/cspell.local.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["global-dict", "local-dict"]
}
